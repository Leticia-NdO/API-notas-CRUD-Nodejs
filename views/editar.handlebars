{{#with nota}}

<form action="/atualizar/{{id}}" method="POST">
    <p>Título: </p>                           
    <input type="text" name="titulo" value="{{titulo}}">
    <p>Conteúdo: </p>
    <textarea name="conteudo">{{conteudo}}</textarea>
    <button type="submit">Atualizar nota</button>
</form>

{{/with}}
